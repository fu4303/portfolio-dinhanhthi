{
  "name": "dinhanhthi.com",
  "version": "4.5.1",
  "license": "MIT",
  "description": "A personal website for note-taking built based on 11ty engine with high performance.",
  "scripts": {
    "re-install": "npx rimraf ./node_modules && npx rimraf ./package-lock.json && npm i",
    "build": "npx rimraf -rf _site && npm run js:build && npm run css:build && eleventy && mkdir -p _live && rm -r _live/* && cp -Rf _site/* _live/",
    "local:watch": "ELEVENTY_ENV=local concurrently \"eleventy --serve --output _built\" \"npm run js:build-watch\" \"npm run css:watch-local\"",
    "local:serve": "cp -Rf _site/* _live/ && http-server _live -p 8081",
    "css:build": "node-sass src/css/main.scss -o src/css",
    "css:build-local": "node-sass --source-comments src/css/main.scss -o src/css",
    "css:watch-local": "node-sass --source-comments true src/css/main.scss -w -o src/css",
    "js:build": "rollup -c src/rollup.config.js",
    "js:build-watch": "rollup -c src/rollup.config.js -w",
    "debug": "DEBUG=* eleventy",
    "share:build": "npm run js:build && npm run css:build && eleventy",
    "share:local-watch": "ELEVENTY_ENV=local concurrently \"eleventy --serve\" \"npm run js:build-watch\" \"npm run css:watch-local\"",
    "site:build": "npm run build",
    "site:update": "sh ud_site.sh",
    "site:build-update": "npm run site:build && npm run site:update"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dinhanhthi/dinhanhthi.com.git"
  },
  "bugs": {
    "url": "https://github.com/dinhanhthi/dinhanhthi.com/issues"
  },
  "homepage": "https://dinhanhthi.com",
  "devDependencies": {
    "@11ty/eleventy": "1.0.0-canary.38",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@gerhobbelt/markdown-it-inline-text-color": "^1.0.1-1",
    "cross-env": "^7.0.3",
    "elasticlunr": "^0.9.5",
    "eleventy-plugin-local-images": "^0.4.0",
    "file-type": "^12.0.1",
    "fs-extra": "^8.1.0",
    "jsdom": "^15.1.1",
    "luxon": "^1.21.3",
    "markdown-it-anchor": "^5.2.5",
    "markdown-it-container": "^3.0.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-kbd": "^2.2.0",
    "markdown-it-task-lists": "^2.1.1",
    "node-fetch": "^2.6.1",
    "pug": ">=3.0.1",
    "rimraf": "^2.7.1",
    "shorthash": "0.0.2"
  },
  "dependencies": {
    "@ampproject/toolbox-optimizer": "^2.7.6",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@saschazar/wasm-avif": "^1.0.0",
    "clean-css": "^4.2.3",
    "concurrently": "^5.2.0",
    "copyfiles": "^2.4.1",
    "core-js": "^3.12.1",
    "csp-hash-generator": "0.0.3",
    "csso": "^4.0.3",
    "datauri": "^3.0.0",
    "eleventy-plugin-nesting-toc": "^1.2.0",
    "expect.js": "^0.3.1",
    "hasha": "^5.2.0",
    "html-minifier": "^4.0.0",
    "image-size": "^0.8.3",
    "jimp": "^0.16.1",
    "katex": "^0.13.0",
    "lru-cache": "^5.1.1",
    "markdown-it": "^11.0.1",
    "markdown-it-attrs": "^3.0.3",
    "markdown-it-emoji": "^2.0.0",
    "markdown-it-heading-wrapper": "github:mozilla/markdown-it-heading-wrapper",
    "markdown-it-mark": "^3.0.0",
    "markdown-it-table-of-contents": "^0.5.0",
    "node-sass": "^5.0.0",
    "phin": "^3.5.0",
    "postcss": "^8.2.10",
    "purge-from-html": "^1.0.3",
    "purgecss": "^4.0.3",
    "querystring": "^0.2.0",
    "rollup": "^2.45.1",
    "sharp": "^0.28.2",
    "twitter": "^1.7.1",
    "ws": "^7.4.6"
  }
}
